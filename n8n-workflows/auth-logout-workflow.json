    },
    {
      "parameters": {
        "jsCode": "// Vérifier si une session a été supprimée\nconst deleteResult = $input.first().json;\n\nif (!deleteResult || !deleteResult.id) {\n  return {\n    json: {\n      success: false,\n      error: 'Session non trouvée ou déjà expirée',\n      deletedCount: 0\n    }\n  };\n}\n\nreturn {\n  json: {\n    success: true,\n    message: 'Déconnexion réussie',\n    deletedSessionId: deleteResult.id\n  }\n};"
      },

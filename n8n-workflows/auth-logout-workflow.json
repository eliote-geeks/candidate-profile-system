    {
      "parameters": {
        "jsCode": "// Extraire le token du header Authorization\nconst headers = $input.item.json.headers;\nconst authHeader = headers.authorization || headers.Authorization;\n\nif (!authHeader) {\n  return {\n    json: {\n      success: false,\n      error: 'Token manquant. Header Authorization requis'\n    }\n  };\n}\n\n// Extraire le token (format: \"Bearer <token>\")\nlet token = authHeader;\nif (authHeader.startsWith('Bearer ')) {\n  token = authHeader.substring(7);\n}\n\nif (!token || token.trim() === '') {\n  return {\n    json: {\n      success: false,\n      error: 'Token invalide'\n    }\n  };\n}\n\nreturn {\n  json: {\n    token: token.trim(),\n    validationPassed: true\n  }\n};"
      },
      "id": "code-extract-token",
      "name": "Extract Token",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [460, 300]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "DELETE FROM sessions WHERE access_token = $1 RETURNING id",
        "options": {
          "queryParameters": "={{ [$json.token] }}"
        }
      },
      "id": "postgres-delete-session",
      "name": "Delete Session",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.4,
